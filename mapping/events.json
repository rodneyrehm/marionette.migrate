{
  "_global_": {

    -- ambiguous
    "close": "destroy",

    "before:item:rendered": "before:render",
    "composite:before:render:collection": "before:render:collection",
    "composite:collection:rendered": "render:collection",
    "collection:before:destroy": "before:destroy:collection",
    "collection:destroy": "destroy:collection",
    "before:child:added": "before:add:child",
    "after:child:added": "add:child",
    "before:child:remove": "before:remove:child",
    "child:removed": "remove:child",



    "itemview:*": "childview:"
  }
}